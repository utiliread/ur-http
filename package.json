{
  "name": "@utiliread/http",
  "version": "1.15.3",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "sideEffects": [
    "./dist/*/json.js",
    "./dist/*/msgpack.js",
    "./src/*/json.ts",
    "./src/*/msgpack.ts"
  ],
  "scripts": {
    "build": "tsc && tsc -p tsconfig.cjs.json && replace-in-file ./http-builder @utiliread/http dist/esm/json.d.ts && replace-in-file ./http-builder @utiliread/http dist/esm/msgpack.d.ts",
    "test": "karma start --single-run --browsers ChromeHeadless karma.config.js",
    "watch": "karma start --auto-watch --browsers ChromeDebugging karma.config.js",
    "clean": "rimraf dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/chai": "^4.2.16",
    "@types/luxon": "^2.0.5",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.4",
    "karma": "^6.3.8",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-typescript": "^5.5.1",
    "luxon": "^2.1.0",
    "mocha": "^9.1.3",
    "replace-in-file": "^6.3.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.4",
    "@utiliread/json": "^1.0.2",
    "@utiliread/jsonpatch": "^1.0.0",
    "@utiliread/msgpack": "^1.0.1"
  },
  "peerDependencies": {
    "luxon": "^2.1.0",
    "@utiliread/json": "^1.0.2",
    "@utiliread/msgpack": "^1.0.1"
  },
  "peerDependenciesMeta": {
    "@utiliread/json": {
      "optional": true
    },
    "@utiliread/msgpack": {
      "optional": true
    }
  }
}
